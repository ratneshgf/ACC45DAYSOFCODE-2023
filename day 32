/*
isxdigit()

character is a hexadecimal character  or not
*/

#include<stdio.h>
#include<conio.h>
#include<ctype.h>
int main()
{
  char ch;
  printf("Enter any character ");
  scanf("%c",&ch);
  if(isxdigit(ch))
  printf("it is a hexadecimal character");
  else
  printf("it is not a hexadecimal character");
  getch();
  return 0;
}
