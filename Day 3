//strcmp()
#include<stdio.h>
#include<conio.h>
#include<string.h>
int main()
{
	char n1[20],n2[20];
	int a;
	printf("Enter 1st string ");
	gets(n1);
	printf("Enter 2nd string ");
	gets(n2);
	a=strcmp(n1,n2);
	if(a==0)
	printf("Both the strings are same/equal\n");
	else
	  if(a>0)
	  printf("String %s is greater \n",n1);
	  else
	  printf("String %s is greater \n",n2);
	getch();
	return(0);
}
